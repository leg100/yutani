{
  "module": {
  },
  "resource": {
    "aws_security_group": {
      "bastion": {
        "name": "bastion-dev",
        "description": "Security group for the bastion instance",
        "vpc_id": "${var.aws_vpc.main.id}"
      }
    },
    "aws_security_group_rule": {
      "bastion": {
        "type": "ingress",
        "security_group_id": "${aws_security_group.bastion.id}",
        "cidr_blocks": null,
        "from_port": 22,
        "to_port": 22,
        "protocol": "tcp"
      }
    },
    "aws_instance": {
      "bastion": {
        "instance_type": "t2.small",
        "vpc_security_group_ids": [
          "${aws_security_group.bastion.id}"
        ],
        "ami": null,
        "key_name": "louisgarman",
        "tags": {
          "Name": [
            null,
            "bastion",
            "dev"
          ],
          "Role": "bastion",
          "Environment": "dev"
        }
      }
    }
  },
  "output": {
    "aws_security_group.bastion.id": {
      "value": "${var.aws_security_group.bastion.id}"
    }
  },
  "variables": {
    "vpc.aws_vpc.main.id": {
    }
  }
}